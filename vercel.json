{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/callback",
            "dest": "/callback.html"
        },
        {
            "src": "/callback/(.*)",
            "dest": "/callback.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "cleanUrls": true
} 